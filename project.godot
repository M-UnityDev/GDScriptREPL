; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[accessibility]

general/accessibility_support=2

[application]

config/name="GDScREPL"
run/main_scene="res://node.tscn"
run/print_header=false
config/features=PackedStringArray("4.5", "GL Compatibility")
run/low_processor_mode=true
run/delta_smoothing=false
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false

[debug]

settings/physics_interpolation/enable_warnings=false
file_logging/enable_file_logging.pc=false

[display]

window/size/viewport_width=1
window/size/viewport_height=1
window/size/resizable=false
window/energy_saving/keep_screen_on=false
window/subwindows/embed_subwindows=false
window/frame_pacing/android/enable_frame_pacing=false
window/dpi/allow_hidpi=false
display_server/driver="headless"
display_server/driver.windows="headless"
display_server/driver.linuxbsd="headless"
display_server/driver.android="headless"
display_server/driver.ios="headless"
display_server/driver.visionos="headless"
display_server/driver.macos="headless"
window/vsync/vsync_mode=0

[gui]

fonts/dynamic_fonts/use_oversampling=false

[input_devices]

pointing/emulate_mouse_from_touch=false

[network]

tls/enable_tls_v1.3=false

[physics]

2d/physics_engine="Dummy"
3d/physics_engine="Dummy"
common/enable_object_picking=false

[rendering]

occlusion_culling/jitter_projection=false
textures/canvas_textures/default_texture_filter=0
rendering_device/fallback_to_vulkan=false
rendering_device/fallback_to_d3d12=false
rendering_device/fallback_to_opengl3=false
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/compress_with_gpu=false
textures/vram_compression/cache_gpu_compressor=false
shader_compiler/shader_cache/enabled=false
shader_compiler/shader_cache/compress=false
shader_compiler/shader_cache/use_zstd_compression=false
shader_compiler/shader_cache/strip_debug=true
